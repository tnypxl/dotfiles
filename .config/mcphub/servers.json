{
  "mcpServers": {
    "filesystem": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users/arikj/Buckle"
      ],
      "autoApprove": [
        "list_directory",
        "read_file",
        "read_multiple_files",
        "search_files",
        "directory_tree",
        "get_file_info"
      ],
      "command": "npx",
      "disabled_tools": []
    },
    "sequentialthinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "autoApprove": [
        "sequentialthinking"
      ],
      "command": "npx",
      "disabled": false
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"]
    },
    "serena": {
      "command": "uvx",
      "args": ["--from","git+https://github.com/oraios/serena","serena","start-mcp-server"]
    }
  },
  "nativeMCPServers": []
}
